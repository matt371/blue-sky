[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/matt371
[access "refs/for/refs/*"]
	read = group user/matt371
	push = group user/matt371
	label-Code-Review = -2..+2 group user/matt371
	label-Verified = -1..+1 group user/matt371
	rebase = group user/matt371
	removeReviewer = group user/matt371
	submit = group user/matt371
[access "refs/heads/*"]
	read = group user/matt371
	create = group user/matt371
	push = +force group user/matt371
	pushMerge = group user/matt371
[access "refs/tags/*"]
	push = group user/matt371
	push = +force group user/matt371
